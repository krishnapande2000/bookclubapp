<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">




    <ImageButton
        android:id="@+id/starter_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"

        app:srcCompat="@drawable/w605085"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/click_anywhere_to_begin_your_journey"
        android:textColor="#DACDCD"
        android:textColorHighlight="#EEE1E1"
        app:layout_constraintBottom_toBottomOf="@+id/starter_button"
        app:layout_constraintEnd_toEndOf="@+id/starter_button"
        app:layout_constraintStart_toStartOf="@+id/starter_button"
        app:layout_constraintTop_toTopOf="@+id/starter_button" />
</androidx.constraintlayout.widget.ConstraintLayout>